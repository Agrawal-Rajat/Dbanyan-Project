# Dbanyan Group Frontend - Environment Variables
# High-performance configuration for production deployment

# API Configuration - FastAPI Backend
VITE_API_BASE_URL=http://localhost:8000/api/v1

# App Configuration
VITE_APP_NAME=Dbanyan Group
VITE_APP_DESCRIPTION=Premium Moringa Products - Natural Health & Wellness

# Performance Configuration
VITE_ENABLE_DEVTOOLS=true
VITE_API_TIMEOUT=10000
VITE_CACHE_DURATION=300000

# Payment Gateway (Razorpay)
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id_here

# Features
VITE_ENABLE_CHATBOT=false
VITE_ENABLE_ANALYTICS=false

# SEO & Social
VITE_SITE_URL=https://dbanyan.com
VITE_SOCIAL_IMAGE=https://dbanyan.com/og-image.jpg

# Development
VITE_DEBUG=true
VITE_LOG_LEVEL=info
